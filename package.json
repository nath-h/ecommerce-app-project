{
  "name": "my-ecommerce-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd ecommerce-frontend && npm run dev",
    "dev:backend": "cd ecommerce-backend && npm run dev",
    "studio": "cd ecommerce-backend && npx prisma studio",
    "build": "cd ecommerce-frontend && npm run build",
    "preview": "cd ecommerce-frontend && npm run preview",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "setup": "npm run setup:install && npm run setup:env && npm run setup:docker && npm run setup:database",
    "setup:install": "npm install && cd ecommerce-frontend && npm install && cd ../ecommerce-backend && npm install",
    "setup:env": "cd ecommerce-backend && cp .env.example .env",
    "setup:docker": "docker-compose up -d",
    "setup:database": "cd ecommerce-backend && npx prisma generate && npx prisma db push && npx prisma db seed"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "overrides": {
    "http-proxy": "npm:http-proxy-3@^1.22.0"
  }
}
